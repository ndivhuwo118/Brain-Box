<div id="winner_<%= dom_id(game)%>" >
  <%= turbo_stream_from "winner_#{dom_id(game)}" %>
  <% if game.complete? %>
    <%= link_to 'The champion is..', final_game_path(game), class: 'btn btn-danger', role: 'button' %>
  <% else  %>
    <h3>Waiting for your opponent to strike...</h3>
  <% end %>
  <%# raise %>
</div>
